<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="660.0" prefWidth="1100.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.edu.neu.java_fundamental.controllers.RenewPasswordController">
   <children>
      <Label prefHeight="57.0" prefWidth="175.0" text="修改密码">
         <font>
            <Font size="28.0" />
         </font>
      </Label>
      <Label layoutX="279.0" layoutY="136.0" prefHeight="57.0" prefWidth="175.0" text="旧密码：">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="279.0" layoutY="231.0" prefHeight="57.0" prefWidth="175.0" text="新的密码：">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="279.0" layoutY="330.0" prefHeight="57.0" prefWidth="175.0" text="确认密码：">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="originpwd" layoutX="454.0" layoutY="143.0" prefHeight="43.0" prefWidth="365.0" />
      <TextField fx:id="newpwd" layoutX="454.0" layoutY="244.0" prefHeight="43.0" prefWidth="365.0" promptText="长度6位以上">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <TextField fx:id="confirmpwd" layoutX="454.0" layoutY="337.0" prefHeight="43.0" prefWidth="365.0" promptText="重新输入一遍新密码">
         <font>
            <Font size="18.0" />
         </font></TextField>
      <Button fx:id="changebtn" layoutX="467.0" layoutY="459.0" mnemonicParsing="false" onAction="#dochange" prefHeight="57.0" prefWidth="165.0" text="确认修改">
         <font>
            <Font size="18.0" />
         </font></Button>
   </children>
</AnchorPane>
